}
.uni-tab-bar {
		background-color: #FFFFFF;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		    -ms-flex-direction: column;
		        flex-direction: column;
		overflow: hidden;
		height: 100%;
}
.uni-tab-bar .list {
		width: 750rpx;
		height: 100%;
}
.uni-swiper-tab {
		width: 100%;
		line-height: 100rpx;
		border-bottom: 1rpx solid #c8c7cc;
		background-color: #FFFFFF;
		text-align: center;
}
.swiper-tab-list {
		width:50%;
		font-size: 30rpx;
        display:inline-block;
		text-align: center;
		color: black;
}
.swiper-tab-list ._img{
		width:65rpx;
		height:65rpx;
		margin-right:20rpx;
		margin-bottom:20rpx;
		vertical-align:middle;
}
.uni-tab-bar .active {
		color: #007AFF;
		border-bottom: 2rpx solid #007AFF;
}
.list{
	background-color: 	#F5F5F5;
}
